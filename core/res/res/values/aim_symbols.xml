<resources>

    <!-- Shutdown/reboot screen -->
    <java-symbol type="layout" name="shutdown_layout" />
    <java-symbol type="color" name="shutdown_background" />
    <java-symbol type="color" name="shutdown_primary_text" />
    <java-symbol type="color" name="shutdown_progress_tint" />
    <java-symbol type="dimen" name="shutdown_margin" />
    <java-symbol type="dimen" name="shutdown_logo_margin_top" />
    <java-symbol type="dimen" name="shutdown_primary_text_margin_top" />
    <java-symbol type="id" name="shutdown_logo" />
    <java-symbol type="id" name="shutdown_message" />
    <java-symbol type="id" name="shutdown_progress" />

    <!-- cpu boost on boot -->
    <java-symbol type="bool" name="config_enableCpuBoostOnBoot" />
    <java-symbol type="integer" name="onbootboost_duration" />
    <java-symbol type="array" name="onbootboost_param_value" />

    <!-- Automatic brightness enhancements -->
    <java-symbol type="integer" name="config_autoBrightnessBrighteningLightFastDebounce"/>

    <!-- On-The-Go -->
    <java-symbol type="string" name="global_action_onthego" />
    <java-symbol type="drawable" name="ic_lock_onthego" />
   
    <!-- Show battery fully charged notification -->
    <java-symbol type="bool" name="config_showBatteryFullyChargedNotification" />
    <java-symbol type="string" name="notify_battery_fully_charged_title" />
    <java-symbol type="string" name="notify_battery_fully_charged_text" />

</resources>
